    "list-context.image-list#home-videos": {
        "children": ["slider-layout#home-videos"],
        "props": {
            "preload": true,
            "height": 650,
            "images": [
                {
                    "image": "https://i.imgur.com/oY7YEDM.jpeg",
                    "description": "image-1",
                    "link": {
                        "url": "/categoria-1"
                    }
                },
                {
