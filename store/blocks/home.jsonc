    "flex-layout.row#deals": {
        "props": {
            "blokClass": "deals"
        },
        "children": [
            "slider-layout#deals-images"
        ]
    },

    "slider-layout#deals-images": {
        "props": {
            "itemsPerPage": {
              "desktop": 5,
              "tablet": 3,
              "phone": 1
            },
            "infinite": true
        }
          
    },
    
    "flex-layout.row#categories": {
        "props": {
            "blokClass": "categories"
        },
        "children": [
            "flex-layout.col#categories-text",
            "flex-layout.col#categories-bubbles-container"
