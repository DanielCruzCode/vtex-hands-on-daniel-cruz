                    }
                }
                // {
                //     "Video": "video#1",
                //     "Link": "link#video-1"
                // }
            ]
        }
    },

    "slider-layout#home-videos": {
        "children": ["link#video-1"],
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true,
            "autoplay": {
                "timeout": 3500
            }
        }
    },

    "link#video-1": {
        "children": ["video#1"],
        "props": {
            "href": "/video-prueba",
            "label": "Video link"
        }
    },

    "video#1": {
        "props": {
            "width": "100%",
            "height": "600px",
            "loop": false,
            "autoPlay": true,
            "muted": false,
            "src": "https://www.youtube.com/watch?v=ZmDBbnmKpqQ"
        }
    }
