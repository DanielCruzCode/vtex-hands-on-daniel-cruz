    "slider-layout#wrapper": {
        "props": {
            "itemsPerPage": {
              "desktop": 1,
              "tablet": 1,
              "phone": 1
            },
            "infinite": true
        },
        "children": ["slider-layout#home-videos"]
    },
    
    "slider-layout#home-videos": {
        "props": {
            "itemsPerPage": {
              "desktop": 1,
              "tablet": 1,
              "phone": 1
            },
            "infinite": true
        },
        "children": ["link#video-1"]
    },

    "link#video-1": {
        "children": [
            "video#1"
        ],
        "props": {
          "href": "/video-prueba",
          "label": "Video link"
        }
    },

    "video#1": {
        "props": {
          "width": "100%",
          "height": "600px",
          "loop": false,
          "autoPlay": true,
          "muted": false,
          "src": "https://www.youtube.com/watch?v=ZmDBbnmKpqQ"
        }
    },

    "slider-layout#home-slider": {
        "props": {
            "infinite": true,
            "showNavigationArrows": "desktopOnly"
          },
          "children": ["image#1", "image#2", "image#3"]
    },

    "image#1":{
        "title": "image-1",
        "props": {
            "link": {
                "url": "/categoria-1"
            },
            "blokClass": "banner",
            "src": "https://i.imgur.com/oY7YEDM.jpeg"
        }
    },
    "image#2":{
